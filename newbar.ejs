<script>
    const btn1 = document.getElementById("op-1");
    const btn2 = document.getElementById("op-2");
    const btn3 = document.getElementById("op-3");
    const opt1text = document.getElementById("op-1-text");
    const opt2text = document.getElementById("op-2-text");
    const opt3text = document.getElementById("op-3-text");
    btn1.style.borderBottom = "0.3rem solid #1c2260";
    btn2.style.borderBottom = "none";
    btn3.style.borderBottom = "none";
    opt1text.style.display = "block";
    opt2text.style.display = "none";
    opt3text.style.display = "none";

    btn1.onclick = function(){
        opt1text.style.display = "block";
        opt2text.style.display = "none";
        opt3text.style.display = "none";
        btn1.style.borderBottom = "0.3rem solid #1c2260";
        btn3.style.borderBottom = "none";
        btn2.style.borderBottom = "none";

    }
    btn2.onclick = function(){

        opt1text.style.display = "none";
        opt2text.style.display = "block";
        opt3text.style.display = "none";
        btn1.style.borderBottom = "none";
        btn2.style.borderBottom = "0.3rem solid #1c2260";
        btn3.style.borderBottom = "none";


    }
    btn3.onclick = function(){
        opt1text.style.display = "none";
        opt2text.style.display = "none";
        opt3text.style.display = "block";
        btn1.style.borderBottom = "none";
        btn2.style.borderBottom = "none";
        btn3.style.borderBottom = "0.3rem solid #1c2260";
    }
</script>

