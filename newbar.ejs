                            <script>


                                masterPages["new_bar"] = [];
                                masterGeo["new_bar"] = [];
                                masterDomains["new_bar"] = [];
                                /*  */




                                $(function(){
                                    var $select = $('.customPageURL').selectize({
                                        plugins: ['remove_button'],
                                        create:true,


                                        onItemAdd:function(value,$item) {
                                            masterPages["new_bar"].push(value);
                                        },
                                        onItemRemove:function(value){
                                            var index = masterPages["new_bar"].indexOf(value);
                                            if (index !== -1 ) { masterPages["new_bar"].splice(index,1); }
                                        },
                                        valueField: 'value',
                                        labelField: 'value',
                                        searchField: 'value',


                                    });

                                    var selectedDomain = [];
                                    var $selectDomain = $('#domainnew_bar').selectize({
                                        plugins: ['remove_button'],
                                        create:true,


                                        onInitialize:function() {
                                            this.setValue(selectedDomain);

                                        },

                                        onItemAdd:function(value,$item) {
                                            masterDomains["new_bar"].push(value);
                                        },
                                        onItemRemove:function(value){
                                            var index = masterDomains["new_bar"].indexOf(value);
                                            if (index !== -1 ) { masterDomains["new_bar"].splice(index,1); }
                                        },
                                        valueField: 'value',
                                        labelField: 'value',
                                        searchField: 'value',


                                    });

                                    //var selectizes = $select[0].selectize;

                                    //selectizes.setValue(selected);


                                    var selectedGeo = [];
                                    var $selectGeo = $('.geoTarget').selectize({
                                        plugins: ['remove_button'],
                                        valueField:'alpha-2',
                                        labelField:'name',
                                        searchField:'name',
                                        onFocus:function() {
                                        },
                                        onChange:function(value,label) {

                                            if (value != "") {
                                                var arr = value.split(',');
                                                for (var i=0; i < arr.length;i++) {
                                                    var name = $('[data-value="' + arr[i] + '"]').text();
                                                    name = name.substr(0,name.length-1);
                                                    arr[i] = {'alpha-2':arr[i], 'name':name};

                                                }
                                                masterGeo["new_bar"] = arr;
                                            }
                                            else {
                                                masterGeo["new_bar"] = [];
                                            }

                                        },
                                        load:function(query, callback) {
                                            if (!query.length) return callback();
                                            $.ajax({
                                                url: 'https://apphero.co/templates/countries.json',
                                                type: 'GET',
                                                dataType: 'json',
                                                data: {
                                                    name: query,
                                                },
                                                error: function() {
                                                    callback();
                                                },
                                                success: function(res) {
                                                    callback(res);
                                                }
                                            });
                                        }
                                    });


                                });
                            </script>
